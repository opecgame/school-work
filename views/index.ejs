  <div id="js-preloader" class="js-preloader">
    <div class="preloader-inner">
      <span class="dot"></span>
      <div class="dots">
        <span></span>
        <span></span>
        <span></span>
      </div>
    </div>
  </div>
  <header class="header-area header-sticky wow slideInDown" data-wow-duration="0.75s" data-wow-delay="0s">
    <div class="container">
      <div class="row">
        <div class="col-12">
          <nav class="main-nav">
            <a class="logo">
              <img src="images/pkwlogo.png" alt="PKW Logo" height="65px" width="60px">
            </a>
            <ul class="nav">
              <li class="scroll-to-section"><a href="#top" class="active">หน้าแรก</a></li>
              <li class="scroll-to-section"><a href="#member">สมาชิกในกลุ่ม</a></li>
              <li>
                <% if (member) { %>
